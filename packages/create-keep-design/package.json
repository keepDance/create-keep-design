{
  "name": "create-keepdesign",
  "version": "1.0.0",
  "description": "一个快速生成组件库搭建环境的脚手架",
  "main": "./dist/index.js",
  "bin": {
    "create-keepdesign": "./dist/index.js"
  },
  "type": "module",
  "scripts": {
    "dev": "vite-node src/",
    "start": "npx",
    "typecheck": "tsc -p ./tsconfig.json --emitDeclarationOnly --declaration",
    "build": "rm -rf dist && tsup src/ --format esm --treeshake",
    "prepack": "pnpm build"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "keep-design",
    "vue3组件库",
    "模板",
    "脚手架",
    "components",
    "create-keep-design"
  ],
  "author": "dshuais",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/keepDance/create-keep-design"
  },
  "homepage": "https://create-keep-design.dshuais.com/",
  "dependencies": {
    "chalk": "^5.3.0",
    "command-line-args": "^5.2.1",
    "command-line-usage": "^7.0.1",
    "download-git-repo": "^3.0.2",
    "node-fetch": "^3.3.2",
    "ora": "^8.0.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "tsup": "^8.0.2",
    "vite-node": "^1.4.0"
  }
}